#include <iostream>
#include <string>

using namespace std;

int main()
{

	string s;
	cin >> s;
	int leftindex = 0;
	int rightindex = s.length() - 1;
	bool isPalindrome = true;
	
	while (leftindex < rightindex)
	{
     	if (s[leftindex] != s[rightindex])
		{
			isPalindrome = false;
			break;
		}
		leftindex++;
		rightindex--;
	}
		if (isPalindrome)
		{
			cout << "it is palindrome" << endl;
		}
		else 
		{ 
			cout << "it is not palindrome";
	}
		cin.get();

	
}
