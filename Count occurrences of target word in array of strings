#include <iostream>
#include <string>

using namespace std;

int main()
{
	string arr[] = { "apple", "banana", "apple", "orange", "banana", "apple" };
	string sub;
	cin >> sub;
	int size = sizeof(arr) / sizeof(arr[0]);
	int count = 0;

	for (int i = 0; i < size; i++)
	{
		if (arr[i] == sub)
		{
			count++;
		}
	}
	cout << count << endl;
	cin.get();
}
