#include <iostream>
#include <string>

using namespace std;

int main()
{
	string str = "programming";

	for (int i = 0; i < str.length(); i++)
	{
		int count = 0;
		for (int j = str.length() - 1; j >= 0; j--)
		{
			if (str[i] == str[j]) {
				count++;
				if (count > 1) {
					break;
				}
			}
		}
		if (count == 1) {

			cout << str[i] << endl;
			break;
		}
	}
	return 0;
}
